<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="{{ asset('bundles/tssearch/css/bootstrap.css') }}" media="screen">
    <link rel="stylesheet" href="{{ asset('bundles/tssearch/css/main.css') }}">

</head>

    <body role="document">

        <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-4">
                        {{ include('TsSearchBundle:Default:searchform.html.twig') }}
                    </div>
                </div>
            </div>
        </div>

        <div class="container searchresults" role="main">

            {% for document in documents %}
                <div class="row">
                    <div class="col-sm-9">
                        <p>Url: {{ document.url }}</p>
                        <p>Eingehende Links: {{ document.incominglinkurls_sm|length }}</p>

                        <ul>
                            {% for link in document.incominglinkurls_sm|sort %}
                                <li>{{ link }}</li>
                            {% endfor %}
                        </ul>
                   </div>
                </div>
            {% endfor %}
        </div>
        <script src="https://code.jquery.com/jquery.js"></script>
        <script src="{{ asset('bundles/tssearch/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/tssearch/js/docs.min.js') }}"></script>

    </body>
</html>